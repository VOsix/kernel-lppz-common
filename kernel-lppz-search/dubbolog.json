{
  "template" : "dubbo-invoke-*",
  "settings" : {
    "index.refresh_interval" : "5s",
    "index.number_of_shards" : 21,  
    "number_of_replicas" : 0
  },
  "mappings" : {
    "_default_" : {
      "_all" : {"enabled" : true, "omit_norms" : true},
      "properties" : {
        "service" : {  
            "type" : "string",  
            "index" : "not_analyzed",
            "doc_values" : true
          },  
          "method" : {  
            "type" : "string",  
            "index" : "not_analyzed",
            "doc_values" : true  
          },  
          "consumer" : {  
            "type" : "string",  
            "index" : "not_analyzed",
            "doc_values" : true  
          },  
          "provider" : {  
            "type" : "string",
            "index" : "not_analyzed",
            "doc_values" : true 
          },  
          "type" : {  
            "type" : "string",  
            "index" : "not_analyzed",
            "doc_values" : true 
          },  
          "success" : {  
            "type" : "double",  
            "index" : "not_analyzed",
            "doc_values" : true  
          },  
          "failure" : {  
            "type" : "double",
            "index" : "not_analyzed",
            "doc_values" : true  
          },  
          "elapsed" : {  
        	   "type" : "double",
            "index" : "not_analyzed",
            "doc_values" : true  
          },  
          "concurrent" : {  
        	  "type" : "integer",
        	  "index" : "not_analyzed",
        	  "doc_values" : true  
          },  
          "invokeDateTime" : {  
        	  "type" : "date",
        	  "format": "yyyy-MM-dd HH:mm:ss",
        	  "index" : "not_analyzed",
        	  "doc_values" : true  
          },  
          "maxElapsed" : {  
        	  "type" : "integer",
        	  "index" : "not_analyzed",
        	  "doc_values" : true  
          },  
          "maxConcurrent" : {  
        	  "type" : "integer",
        	  "index" : "not_analyzed",
        	  "doc_values" : true  
          },  
          "invokeTime" : {  
        	  "type" : "long",
        	  "index" : "not_analyzed",
        	  "doc_values" : true  
          },
          "exception" : {  
        	  "type" : "string",
        	  "index" : "not_analyzed"
          },
          "exceptionClazz" : {  
        	  "type" : "string",
        	  "index" : "not_analyzed"
          },
          "hostUri" : {  
        	  "type" : "string",
        	  "index" : "not_analyzed"
          },
          "requestBody" : {  
        	  "type" : "string",
        	  "index" : "not_analyzed"
          },
          "responseBody" : {  
        	  "type" : "string",
        	  "index" : "not_analyzed"
          },
          "httpMethodType" : {  
        	  "type" : "string",
        	  "index" : "not_analyzed"
          }
      }
    }
  }
}